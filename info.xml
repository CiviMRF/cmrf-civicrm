<?xml version="1.0"?>
<extension key="cmrf-civicrm" type="module">
  <file>cmrf_civicrm</file>
  <name>CMRF CiviCRM</name>
  <description>Access remote CiviCRM instances with rest calls</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Klaas Eikelboom</author>
    <email>klaas.eikelboom@civicoop.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/CiviMRF/cmrf-civicrm</url>
    <url desc="Documentation">https://github.com/CiviMRF/cmrf-civicrm/blob/default/README.md</url>
    <url desc="Support">https://github.com/CiviMRF/cmrf-civicrm/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-05-19</releaseDate>
  <version>0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/CmrfCivicrm</namespace>
  </civix>
</extension>
